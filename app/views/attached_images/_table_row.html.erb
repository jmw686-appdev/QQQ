<tr>
  <td>
    <%= attached_image.image_id %>
  </td>

  <td>
    <%= attached_image.response_id %>
  </td>

  <td>
    <%= attached_image.question_id %>
  </td>

  <td>
    <div class="btn-group">
      <%= link_to attached_image, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-search-plus fa-fw"></i>
      <% end %>

      <%= link_to edit_attached_image_url(attached_image), class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-edit fa-fw"></i>
      <% end %>

      <%= link_to attached_image, method: :delete, data: { confirm: "Do you really want to delete this attached_image?" }, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-trash-alt fa-fw"></i>
      <% end %>
    </div>
  </td>
</tr>
