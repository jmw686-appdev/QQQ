<tr>
  <td>
    <%= question_tag.tag_id %>
  </td>

  <td>
    <%= question_tag.question_id %>
  </td>

  <td>
    <div class="btn-group">
      <%= link_to question_tag, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-search-plus fa-fw"></i>
      <% end %>

      <%= link_to edit_question_tag_url(question_tag), class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-edit fa-fw"></i>
      <% end %>

      <%= link_to question_tag, method: :delete, data: { confirm: "Do you really want to delete this question_tag?" }, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-trash-alt fa-fw"></i>
      <% end %>
    </div>
  </td>
</tr>
